<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
		<setting name="callSettersOnNulls" value="true"/>
	</settings>
	<typeAliases>
		<typeAlias type="com.uni.spring.employee.model.dto.Employee" alias="Employee"/>
		<typeAlias type="com.uni.spring.approval.model.dto.Approval" alias="Approval"/>
		<typeAlias type="com.uni.spring.employee.model.dto.WorkingDay" alias="WorkingDay"/>
		<typeAlias type="com.uni.spring.common.dto.Attachment" alias="Attachment"/>
		<typeAlias type="com.uni.spring.schedule.model.dto.Schedule" alias="Schedule"/>
		<typeAlias type="com.uni.spring.schedule.model.dto.ScheduleDTO" alias="ScheduleDTO"/>
		<typeAlias type="com.uni.spring.employee.model.dto.TimeOff" alias="TimeOff"/>
		<typeAlias type="com.uni.spring.employee.model.dto.TimeOffContent" alias="TimeOffContent"/>
		<typeAlias type="com.uni.spring.hr.model.dto.Hr" alias="Hr"/>
		<typeAlias type="com.uni.spring.employee.model.dto.Department" alias="Department"/>
		<typeAlias type="com.uni.spring.mail.model.dto.Mail" alias="Mail"/>
		<typeAlias type="com.uni.spring.mail.model.dto.ReceiveMail" alias="ReceiveMail"/>
		<typeAlias type="com.uni.spring.employee.model.dto.JobPosition" alias="JobPosition"/>
		<typeAlias type="com.uni.spring.approval.model.dto.Opinion" alias="Opinion"/>
		<typeAlias type="com.uni.spring.notice.model.dto.Notice" alias="Notice"/>
	</typeAliases>
	<mappers>
		<mapper resource="/mappers/employee-mapper.xml"/>
		<mapper resource="/mappers/approval-mapper.xml"/>
		<mapper resource="/mappers/schedule-mapper.xml"/>
		<mapper resource="/mappers/manager-mapper.xml"/>
		<mapper resource="/mappers/mail-mapper.xml"/>
		<mapper resource="/mappers/notice-mapper.xml"/>
	</mappers>
</configuration>